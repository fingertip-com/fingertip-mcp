{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "14"
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-optional-chaining",
    [
      "module-resolver",
      {
        "root": ["./dist"]
      }
    ]
  ]
}
